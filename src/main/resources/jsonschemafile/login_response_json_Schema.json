{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "session": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "value"
      ]
    },
    "loginInfo": {
      "type": "object",
      "properties": {
        "failedLoginCount": {
          "type": "integer"
        },
        "loginCount": {
          "type": "integer"
        },
        "lastFailedLoginTime": {
          "type": "string"
        },
        "previousLoginTime": {
          "type": "string"
        }
      },
      "required": [
        "failedLoginCount",
        "loginCount",
        "lastFailedLoginTime",
        "previousLoginTime"
      ]
    }
  },
  "required": [
    "session",
    "loginInfo"
  ]
}